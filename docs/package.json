    "clsx": "^1.1.1",
    "mdx.macro": "^0.2.9",
    "prism-react-renderer": "^1.3.1",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redocusaurus": "^1.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.20"
  },
